//https://leetcode.com/problems/plus-one/
class Solution {
    public int[] plusOne(int[] digits) {

        int i=digits.length;
        for(i=i-1;i >=0;i--){
            if(digits[i] < 9){
                digits[i]++;
                return digits;
            }
            digits[i]=0;
        }
        int increased_number[]=new int[digits.length + 1];
        increased_number[0]=1;
        return increased_number;        
    }
}
